}

.frame1{
  position: fixed;
  left: 10%;
  top: 25%;
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 80%;
  height: 25%;
  background-color: springgreen;
}

.frame2{
  background-color:pink;
  position: fixed;
  left: 10%;
  top: 55%;
